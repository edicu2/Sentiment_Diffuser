<?xml version="1.0" encoding="utf-8"?>

<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <FrameLayout
        android:id="@+id/frameLayout"
        android:layout_width="match_parent"
        android:layout_height="104dp"
        android:layout_marginTop="50dp"
        android:background="@drawable/set_line"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/connect_image"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_gravity="center|left"
            android:layout_marginLeft="20dp"
            app:srcCompat="@drawable/connect" />

        <TextView
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center|center"
            android:layout_marginRight="10dp"
            android:text="Device Connect"
            android:textColor="#fff"
            android:textSize="19dp" />

        <ImageView
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:layout_gravity="center|right"
            android:layout_marginRight="15dp"
            app:srcCompat="@drawable/point" />

    </FrameLayout>

    <FrameLayout
        android:id="@+id/frameLayout2"
        android:layout_width="match_parent"
        android:layout_height="104dp"
        android:background="@drawable/set_line"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/frameLayout">

        <ImageView
        android:id="@+id/interval_image"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_gravity="center|left"
        android:layout_marginLeft="20dp"
        app:srcCompat="@drawable/interval" />

        <TextView
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center|center"
            android:layout_marginRight="10dp"
            android:text="Sentiment interval"
            android:textColor="#fff"
            android:textSize="19dp" />

        <ImageView
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:layout_gravity="center|right"
            android:layout_marginRight="15dp"
            app:srcCompat="@drawable/point" />
    </FrameLayout>

    <FrameLayout
        android:id="@+id/frameLayout3"
        android:layout_width="match_parent"
        android:layout_height="104dp"
        android:background="@drawable/set_line"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/frameLayout2">

        <ImageView
        android:id="@+id/sleep_image"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_gravity="center|left"
        android:layout_marginLeft="20dp"
        app:srcCompat="@drawable/sleep" />

        <TextView
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center|center"
            android:layout_marginRight="10dp"
            android:text="Sentiment stop time"
            android:textColor="#fff"
            android:textSize="19dp" />

        <ImageView
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:layout_gravity="center|right"
            android:layout_marginRight="15dp"
            app:srcCompat="@drawable/point" />

    </FrameLayout>

    <FrameLayout
        android:id="@+id/frameLayout4"
        android:layout_width="match_parent"
        android:layout_height="104dp"
        android:background="@drawable/set_line"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/frameLayout3">

        <ImageView
            android:id="@+id/notification_image"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_gravity="center|left"
            android:layout_marginLeft="20dp"
            app:srcCompat="@drawable/notification" />

        <TextView
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center|center"
            android:layout_marginRight="10dp"
            android:text="Notification"
            android:textColor="#fff"
            android:textSize="19dp" />

        <ImageView
            android:layout_width="40dp"
            android:layout_height="32dp"
            android:layout_gravity="center|right"
            android:layout_marginRight="7dp"
            app:srcCompat="@drawable/toggle" />
    </FrameLayout>

    <FrameLayout
        android:id="@+id/frameLayout5"
        android:layout_width="match_parent"
        android:layout_height="104dp"
        android:layout_marginBottom="110dp"
        android:background="@drawable/set_line"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/frameLayout4"
        app:layout_constraintVertical_bias="0.0">

        <ImageView
            android:id="@+id/guide_image"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_gravity="center|left"
            android:layout_marginLeft="20dp"
            app:srcCompat="@drawable/guide" />

        <TextView
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center|center"
            android:layout_marginRight="10dp"
            android:text="Guidelines"
            android:textColor="#fff"
            android:textSize="19dp" />

        <ImageView
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:layout_gravity="center|right"
            android:layout_marginRight="15dp"
            app:srcCompat="@drawable/point" />

    </FrameLayout>

    <FrameLayout
        android:id="@+id/frameLayout6"
        android:layout_width="match_parent"
        android:layout_height="104dp"
        android:layout_marginBottom="110dp"
        android:background="@drawable/set_line"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/frameLayout5"
        app:layout_constraintVertical_bias="0.0">

        <ImageView
            android:id="@+id/face_image"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_gravity="center|left"
            android:layout_marginLeft="20dp"
            app:srcCompat="@drawable/faceboo" />

        <TextView
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center|center"
            android:layout_marginRight="10dp"
            android:text="FaceBook"
            android:textColor="#fff"
            android:textSize="19dp" />

        <Button
            android:id="@+id/faceBook_Login"
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:layout_gravity="center|right"
            android:layout_marginRight="15dp"
            android:background="@drawable/point" />

    </FrameLayout>

</android.support.constraint.ConstraintLayout>